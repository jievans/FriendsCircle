<h1>Circles#new</h1>

<form action="<%= circles_url %>" method="post">
  <input type="hidden" name="authenticity_token"
    value="<%= form_authenticity_token%>">

  <% User.all.each do |user| %>
    <% html_id = "circle_member_ids_#{user.id}" %>
    <input
      type="checkbox"
      name="circle[member_ids][]"
      value="<%= user.id %>"
      id="<%= html_id%>">
      <label for="<%= html_id%>">
        <%= user.username %>
      </label>
      <br>
  <% end %>
  <br>
  <label for="circle_name">Circle Name</label>
  <input type="text" name="circle[name]" id="circle_name">
  <br>
  <input type="submit">

</form>